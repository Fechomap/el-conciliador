{
  "app": {
    "name": "El Conciliador",
    "version": "1.0.0"
  },
  "server": {
    "port": 3000,
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  },
  "mongodb": {
    "collection_names": {
      "expedientes": "expedientes",
      "clientes": "clientes",
      "logs": "logs"
    },
    "indexes": {
      "expedientes": ["numeroExpediente", "cliente"]
    }
  }
}
